    "react": "^15.6.1",
    "react-ace": "^5.2.0",
    "react-css-modules": "^4.3.0",
    "react-dom": "^15.6.1",
    "react-hammerjs": "^0.5.0",
    "react-measure": "^2.0.2",
    "style-loader": "^0.18.2",
    "svg-path-generator": "^1.1.0",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "dockerode": "^2.5.1",
    "koa": "^2.3.0",

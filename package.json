{
  "name": "DragonASIC",
  "version": "1.0.0",
  "scripts": {
    "build:web": "concurrently \"npm run build:web:html\" \"npm run build:web:js\"",
    "build:web:html": "pug web/index.pug",
    "build:web:js": "cd web && webpack",
    "api": "node api/index.js",
    "lint": "eslint . --ignore-pattern web/index.js",
    "dev:web": "cd web && webpack-dev-server",
    "dev": "concurrently \"npm run dev:web\" \"npm run api\""
  },
  "main": "api/index.js",
  "author": "DragonASIC",
  "license": "MIT",
